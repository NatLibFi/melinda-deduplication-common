{
  "name": "@natlibfi/melinda-deduplication-common",
  "description": "Shared modules for microservices of Melinda deduplication system",
  "homepage": "https://github.com/natlibfi/melinda-deduplication-common-melinda",
  "bugs": {
    "url": "https://github.com/natlibfi/melinda-deduplication-common-melinda/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:natlibfi/melinda-deduplication-common-melinda.git"
  },
  "license": "LGPL-3.0+",
  "version": "1.0.4",
  "main": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "lint": "eslint --fix src",
    "clean:build": "rimraf dist",
    "build": "npm run clean:build && babel src -d dist",
    "test": "npm run lint && npm run test:unit && flow",
    "test:unit": "NODE_ENV=test mocha --compilers js:babel-register src/**/*.spec.js",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel-register src/**/*.spec.js -w",
    "publish-to-npm": "npm install && npm test && npm publish --access=public"
  },
  "dependencies": {
    "@natlibfi/marc-record-merge": "3.3.0",
    "es6-promisify": "^5.0.0",
    "json-similarity": "^1.0.1",
    "lodash": "^4.17.4",
    "marc-record-js": "^0.3.2",
    "marc-record-serializers": "^1.2.1",
    "melinda-api-client": "1.1.0",
    "moment": "^2.18.1",
    "node-fetch": "^1.7.1",
    "node-uuid": "^1.4.8",
    "querystring": "^0.2.0",
    "roman-numerals": "^0.3.2",
    "synaptic": "^1.1.1",
    "unorm": "^1.4.1",
    "uuid": "^3.1.0",
    "winston": "^2.3.1",
    "wuzzy": "^0.1.3",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.1",
    "eslint": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "flow-bin": "^0.61.0",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.2",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0"
  }
}
